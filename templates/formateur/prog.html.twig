{% extends 'base.html.twig' %}

{% block title %}Programme de {{module.matiere.nom}} pour {{module.formation.nom}}{% endblock %}

{% block body %}

<div>
    <h1>Le programme de {{ module.matiere.nom }} pour la formation {{ module.formation.nom }} est : </h1>

{{ module.programme }}

</div>
<br><br>
<div>
    <a href="{{path("editProgramme", {id : module.id}) }}">Modifier le programme</a>
</div>

<button onclick="history.back()">Retourner à la page précédente</button>

{% endblock %}
